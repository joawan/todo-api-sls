{
  "name": "update",
  "runtime": "nodejs",
  "customName": false,
  "customRole": false,
  "handler": "update/handler.handler",
  "timeout": 6,
  "memorySize": 128,
  "custom": {
    "excludePatterns": [],
    "envVars": [ "DYNAMODB_TODO_TABLE", "AWS_REGION" ],
    "optimize": {
        "exclude": ["aws-sdk"]
    }
  },
  "environment": {
    "DYNAMODB_TODO_TABLE": "${envDynamoDBTodoTable}",
    "AWS_REGION": "${region}"
  },
  "endpoints": [
    {
      "path": "todos/{id}",
      "method": "PUT",
      "type": "AWS",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplate}",
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ],
  "events": []
}
